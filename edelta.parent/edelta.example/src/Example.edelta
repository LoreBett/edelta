metamodel "ecore"
metamodel "myexample"
metamodel "myecore"

def createClass(String name) {
	newEClass(name)
}

ecoreref(MyExampleEClass) // from myexample ecore in model project
ecoreref(MyEClass) // from myecore ecore in this project
ecoreref(EAnnotation) // from EMF ecore

ecoreref(myexample).EClassifiers

